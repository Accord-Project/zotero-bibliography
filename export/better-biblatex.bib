@inproceedings{roxinDigitalBuildingTwins2019,
  title = {Digital Building Twins - {{Contributions}} of the {{ANR McBIM}} Project},
  booktitle = {15th {{IEEE International Conference}} on {{Signal Image Technology}} \& {{Internet Based Systems}}, {{SITIS}} 2019},
  author = {Roxin, Ana and Abdou, Wahabou and Ginhac, Dominique and Derigent, William and Dragomirescu, Daniela and Montegut, Laurent},
  editor = {{IEEE}},
  date = {2019-11},
  location = {{Sorrento, Italy}},
  doi = {10.1109/SITIS.2019.00071},
  url = {https://hal.archives-ouvertes.fr/hal-02420387},
  urldate = {2022-08-05},
  abstract = {This paper presents the vision behind the McBIM project, funded by the French National Agency of Research (ANR). McBIM stands for "Communicating Matter for BIM" and is a concept coined by the CRAN laboratory, concept at the basis of the scientific contributions of the project. Building upon latest advances in Semantic Web technologies, Wireless Sensor Networks and digital twins in AEC (Architecture, Engineering and Construction), the project aims at delivering real-time integration of sensor data into accurate digital building representations. The overall goal is to enable actionable knowledge, meaning knowledge that supports decision making. After a brief summary of the issues addressed and the overall project vision, this article depicts main contributions made so far in the context of the ANR McBIM project. These address semantic interoperability with BIM standards (e.g. IFC ISO 16739), routing in wireless sensor networks along with autonomic computing.},
  keywords = {Autonomic Computing,Building Digital Twin,Building Information Modeling,Concrete,IFC,Linked Data,McBIM,Property Sets,Semantic Interoperability,Semantic Web,Wireless Sensor Networks}
}

@article{roxinInteroperableDigitalBuilding2021,
  title = {Interoperable {{Digital Building Twins Through Communicating Materials}} and {{Semantic BIM}}},
  author = {Roxin, Ana and Abdou, Wahabou and Derigent, William},
  date = {2021-10-28},
  journaltitle = {SN Computer Science},
  shortjournal = {SN COMPUT. SCI.},
  volume = {3},
  number = {1},
  pages = {23},
  issn = {2661-8907},
  doi = {10.1007/s42979-021-00860-w},
  url = {https://doi.org/10.1007/s42979-021-00860-w},
  urldate = {2022-08-05},
  abstract = {This paper presents contributions of the ANR McBIM (Communicating Material for BIM) project regarding Digital Building Twins, specifically how Semantic Web technologies allow providing explainable decision-support. Following an introduction stating our understanding of a Digital Building Twin (DBT), namely a lively representation of a buildings' status and environment, we identify five main research domains following the study of main research issues related to DBT. We then present the state-of-the-art and existing standards for digitizing the construction process, Semantic Web technologies, and wireless sensor networks. We further position the main contributions made so far in the ANR McBIM project's context according to this analysis, e.g., sensor placement in the communicating material and explainable decision-support.},
  langid = {english},
  keywords = {Building information modeling (BIM),Concrete,Digital building twin (DBT),IFC,Linked data,McBIM,Semantic interoperability,Semantic web,Sensors,Wireless sensor networks}
}

