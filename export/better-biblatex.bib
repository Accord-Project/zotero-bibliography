@article{fenselSESAMESSemanticSmart2013,
  title = {{{SESAME-S}}: {{Semantic Smart Home System}} for {{Energy Efficiency}}},
  shorttitle = {{{SESAME-S}}},
  author = {Fensel, Anna and Tomic, Slobodanka and Kumar, Vikash and Stefanovic, Milan and Aleshin, Sergey V. and Novikov, Dmitry O.},
  date = {2013-02-01},
  journaltitle = {Informatik-Spektrum},
  shortjournal = {Informatik Spektrum},
  volume = {36},
  number = {1},
  pages = {46--57},
  issn = {1432-122X},
  doi = {10.1007/s00287-012-0665-9},
  url = {https://doi.org/10.1007/s00287-012-0665-9},
  urldate = {2021-05-14},
  abstract = {As the urgent need for efficient and sustainable energy usage becomes ever more apparent, interest in Smart Homes is on the rise. The SESAME-S project (SEmantic SmArt Metering – Services for Energy Efficient Houses) uses semantically linked data to actively assist end-consumers in making well-informed decisions and controlling their energy consumption. By integrating smart metering and home automation functionality, SESAME-S works to effectively address the potential mass market of end-consumers with an easily customizable solution that can be widely implemented in domestic or business environments, with expected savings of over 20\,\% from the total energy bill. The developed system is a basis for conceptualizing, demonstrating, and evaluating a variety of innovative end-consumer services and their user interface paradigms. In this paper, we present the SESAME-S system as a whole and discuss the semantically enabled services, demonstrating that such systems may have broad acceptance in the future. The data obtained through such systems will be invaluable for future global energy-efficiency strategies and businesses.},
  langid = {english}
}

@article{fortineauAutomatedBusinessRules2019,
  title = {Automated Business Rules and Requirements to Enrich Product-Centric Information},
  author = {Fortineau, Virginie and Paviot, Thomas and Lamouri, Samir},
  date = {2019-01-01},
  journaltitle = {Computers in Industry},
  shortjournal = {Computers in Industry},
  volume = {104},
  pages = {22--33},
  issn = {0166-3615},
  doi = {10.1016/j.compind.2018.10.001},
  url = {https://www.sciencedirect.com/science/article/pii/S0166361518301477},
  urldate = {2021-05-12},
  abstract = {Current PLM or BIM based information systems suffer from a lack of checking components for business rules. One reason is the misunderstanding of the role and nature of business rules, and how they should be treated in a product-centric information system. This paper intends to provide both a process and a related model to build such a component and enrich future systems. Rules and requirements process management enables the unambiguous formalization of implicit knowledge contained in business rules, generally expressed in easily understandable language, and leads to the formal expression of requirements. In this paper, the requirements are considered a consequence of the application of a business rule. A conceptual model is then introduced, called DALTON (DAta Linked Through Occurrences Network), which supports this process. In this ontology, concepts and product data, coming for instance from an existing product database, are represented using instances and occurrences, connected together with triples built from business rules and requirements according to previous management processes. An experiment involving a set of SWRL rules is conducted in the Protégé environment that validates the model and the process.},
  langid = {english},
  keywords = {BIM,Business rule,DALTON,DAta Linked Through Occurrences Network,Ontology,PLM,Protege,Requirements,SWRL},
  note = {\begin{itemize}

\item The paper provides an ontological discussion about rules and requirement management.

\item A full rule and requirement process is proposed to achieve business rules integration in a PLM / BIM system.

\item A conceptualization is proposed in order to enable the implementation of business rules and requirements in a PLM/BIM system. The conceptualization is free of any captive technology or language.

\item An ontological application is proposed to prove the concept.

\end{itemize}}
}

