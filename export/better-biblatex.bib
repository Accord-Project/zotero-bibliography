@article{jiangAutomaticBuildingInformation2015,
  title = {Automatic Building Information Model Query Generation},
  author = {Jiang, Yufei and Yu, Nan and Ming, Jiang and Lee, Sanghoon and DeGraw, Jason and Yen, John and Messner, John I. and Wu, Dinghao},
  date = {2015-12-08},
  journaltitle = {Journal of Information Technology in Construction (ITcon)},
  volume = {20},
  number = {30},
  pages = {518--535},
  url = {http://www.itcon.org/paper/2015/30},
  urldate = {2021-05-12},
  abstract = {Energy efficient building design and construction calls for extensive collaboration between different subfields of the Architecture, Engineering and Construction (AEC) community. Performing building design and construction engineering raises challenges on data integration and software interoperability. Using Building Information Modeling (BIM) data hub to host and integrate building models is a promising solution to address those challenges, which can ease building design information management. However, the partial model query mechanism of current BIM data hub collaboration model has several limitations, which prevents designers and engineers to take advantage of BIM. To address this problem, we propose a general and effective approach to generate query code based on a Model View Definition (MVD). This approach is demonstrated through a software prototype called QueryGenerator. By demonstrating a case study using multi-zone air flow analysis, we show how our approach and tool can help domain experts to use BIM to drive building design with less labour and lower overhead cost.},
  langid = {english}
}

@article{jiangCombiningBIMOntology2018,
  title = {Combining {{BIM}} and {{Ontology}} to {{Facilitate Intelligent Green Building Evaluation}}},
  author = {Jiang, Shaohua and Wang, Na and Wu, Jing},
  date = {2018-09-01},
  journaltitle = {Journal of Computing in Civil Engineering},
  volume = {32},
  number = {5},
  pages = {04018039},
  publisher = {{American Society of Civil Engineers}},
  issn = {1943-5487},
  doi = {10.1061/(ASCE)CP.1943-5487.0000786},
  url = {https://ascelibrary.org/doi/abs/10.1061/%28ASCE%29CP.1943-5487.0000786},
  urldate = {2021-05-17},
  abstract = {Green building evaluation is an effective measure to reflect the sustainability performance of buildings and is beneficial to improve the deteriorating environment. However, most evaluation tasks are conducted in a manual way nowadays, which is time-consuming, laborious, and error-prone. Thus, a new approach taking advantage of building information model (BIM) and ontology is proposed in this paper to facilitate the process of green building evaluation. Information-rich BIM serves as the provider of multidisciplinary data required for green building evaluation while green building evaluation ontology (GBEOntology) is used for representing fragmented information included in evaluation standards. The reasoning process is realized in a Jess rule engine combining semantic web rule language (SWRL) rules and ontology web language (OWL) ontology. The proposed approach can save time and realize the reuse of green building evaluation standard knowledge. An implemented test case validates the effectiveness of the proposed approach by looking at the example of Evaluation Standard for Green Building (GB/T50378-2014) of China.},
  langid = {english},
  keywords = {Building information model (BIM),Green building evaluation,Intelligent evaluation,Ontology,Reasoning}
}

