@article{diaz-vilarinoSemanticAsbuilt3d2013,
  title = {Semantic As-Built 3d Models Including Shades for the Evaluation of Solar Influence on Buildings},
  author = {Díaz-Vilariño, L. and Lagüela, S. and Armesto, J. and Arias, P.},
  date = {2013-06-01},
  journaltitle = {Solar Energy},
