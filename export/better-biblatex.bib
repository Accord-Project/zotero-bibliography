@article{eastmanAutomaticRulebasedChecking2009,
  title = {Automatic Rule-Based Checking of Building Designs},
  author = {Eastman, C. and Lee, Jae-min and Jeong, Yeon-suk and Lee, Jin-kook},
  date = {2009-12},
  journaltitle = {Automation in Construction},
  shortjournal = {Automation in Construction},
