@inproceedings{hoogwoutDigitalBuildingPermit2004,
  title = {Digital {{Building Permit Application}}: {{A Feasibility Study}} for a {{Shared Services Solution}} in the {{Netherlands}}},
  shorttitle = {Digital {{Building Permit Application}}},
  booktitle = {Electronic {{Government}} ({{EGOV}} 2004)},
  author = {Hoogwout, Marcel and te Velde, Robbin},
  editor = {Traunmüller, Roland},
  options = {useprefix=true},
  date = {2004},
  series = {Lecture {{Notes}} in {{Computer Science}}},
