    <bibo:Article rdf:about="info:doi/10.1109/ICSC.2017.26">
        <bibo:pages>73-76</bibo:pages>
        <bibo:doi>10.1109/ICSC.2017.26</bibo:doi>
        <bibo:presentedAt>
