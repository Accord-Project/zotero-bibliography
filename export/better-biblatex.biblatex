@inproceedings{oraskariLiveWebOntology2021,
  title = {Live {{Web Ontology}} for {{buildingSMART Data Dictionary}}},
  booktitle = {Forum {{Bauinformatik}}},
  author = {Oraskari, Jyrki},
  date = {2021},
  pages = {166--173},
  url = {https://www.researchgate.net/profile/Jyrki-Oraskari/publication/355425683_Live_Web_Ontology_for_buildingSMART_Data_Dictionary/links/616fcce1718a2a7099e4d86b/Live-Web-Ontology-for-buildingSMART-Data-Dictionary.pdf},
  abstract = {The aspiration of Linked Building Data is in accelerating construction processes, improved collaboration, facilitating facility management, and an interlingua for the BIM domain. Linked Building Data is widely used in research projects and actively advances by the researchers in the World Wide Web Consortium Linked Building Data Community Group. buildingSMART is a worldwide organization that develops standards to enable collaboration and digital BIM workflows. The buildingSMART Data Dictionary (bSDD) is an international standard that defines a hierarchically structured encyclopedia of object concepts and their properties used in building information models. It is provided as is an online service. While there have been earlier work where data exports of bSDD have been expressed as the Web Ontology Language (OWL) and used in linked data, a consistent approach for keeping them automatically up to date has been missing. In this work, I study how to publish the buildingSMART Data Dictionary classifications and terms as a web ontology, use them in the Linked Building Data context, and keep the data in sync with the buildingSMART Data Dictionary primary data. I present an online ontology generator that ad-hoc provides up-to-date ontology descriptions of concepts in the buildingSMART database.The earlier work is presented in this work, and the feasibility of the online live updates for the ontologies that reflect the bSDD database data is shown. The results are reflected and compared with the earlier work. This paper summarizes ways to publish the bSDD vocabularies as linked data in a sustainable way.},
  keywords = {/have,/unread,buildingSmart Data Dictionary (bSDD),Linked Building Data (LBD),Ontology,OWL}
}

@article{oraskariRDFbasedSignatureAlgorithms2015,
  title = {{{RDF-based}} Signature Algorithms for Computing Differences of {{IFC}} Models},
  author = {Oraskari, Jyrki and Törmä, S.},
  date = {2015},
  doi = {10.1016/J.AUTCON.2015.05.008},
  abstract = {The capability to accurately detect changes between successive versions of the IFC representation of a BIM model would enable the development of generic change management functionalities for construction projects. Unfortunately, IFC models consist mostly of anonymous objects without stable identities; therefore the computation of differences between versions is complicated. When IFC models are converted into an RDF representation, the uniform graph structure offers new algorithmic opportunities. We study how to assign unique and stable identities to anonymous nodes based on signatures generated from their graph environment, and present the Short Paths Crossings Algorithm (SPCA) that computes sets of paths with limited length from anonymous nodes taking into account their crossings. Empirical tests show that SPCA produces significantly smaller difference sets for IFC-derived graphs than previous algorithms for RDF change detection.},
  keywords = {/unread,Building Information Modeling (BIM),Diff,International Foundation Classes (IFC),RDF}
}

