}

@inproceedings{riguzziBUNDLEReasonerProbabilistic2013,
  title = {{{BUNDLE}}: {{A Reasoner}} for {{Probabilistic Ontologies}}},
  shorttitle = {{{BUNDLE}}},
  booktitle = {Web {{Reasoning}} and {{Rule Systems}}},
  author = {Riguzzi, Fabrizio and Bellodi, Elena and Lamma, Evelina and Zese, Riccardo},
  editor = {Faber, Wolfgang and Lembo, Domenico},
  date = {2013},
  series = {Lecture {{Notes}} in {{Computer Science}}},
  pages = {183--197},
  publisher = {{Springer}},
  location = {{Berlin, Heidelberg}},
  doi = {10.1007/978-3-642-39666-3_14},
  abstract = {Representing uncertain information is very important for modeling real world domains. Recently, the DISPONTE semantics has been proposed for probabilistic description logics. In DISPONTE, the axioms of a knowledge base can be annotated with a set of variables and a real number between 0 and 1. This real number represents the probability of each version of the axiom in which the specified variables are instantiated. In this paper we present the algorithm BUNDLE for computing the probability of queries from DISPONTE knowledge bases that follow the \$\textbackslash mathcal\{ALC\}\$semantics. BUNDLE exploits an underlying DL reasoner, such as Pellet, that is able to return explanations for queries. The explanations are encoded in a Binary Decision Diagram from which the probability of the query is computed. The experiments performed by applying BUNDLE to probabilistic knowledge bases show that it can handle ontologies of realistic size and is competitive with the system PRONTO for the probabilistic description logic P-\$\textbackslash mathcal\{SHIQ\}\$(D).},
  isbn = {978-3-642-39666-3},
  langid = {english},
  keywords = {Binary Decision Diagram,Description Logic,Expansion Rule,Logic Program,Predicate Logic}
}

