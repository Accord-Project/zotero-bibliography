@report{busBIM4RenD4Object2022,
  title = {{{BIM4Ren}} - {{D4}}.6 {{Object}} Catalog Online Service and Backends Application},
  author = {Bus, Nicolas and Gasiglia, Baptiste and Fies, Bruno and Pastorelly, Nicolas},
  date = {2022-09-21},
  pages = {26},
  institution = {{Nobatek/INEF4}},
  url = {https://bim4ren.eu/download/d4-6-object-catalog-online-service-and-backends-application/},
  abstract = {The  Energy  retrofitting  of  the  European  Building  stock  appears  to  be  one  of  the  most  effective measures  to  accomplish  the  EU  Objectives  in  terms  of  sustainability.  The  Energy  retrofitting  offers meaningful and tangible benefits beyond energy cost saving, such as the reduction of C02 emissions and  demolition  wastes  or  others  which  give  added  valueto  the  building  such  as  comfort  for  its inhabitants.On the other hand, the common complexity and low cost-efficiency of the renovation process often make the retrofitting unaffordable. In addition, the renovation process is a sequence of activities, withmany interlinked dependencies between several stakeholders, usually in a very fragmented market. All these drawbacks hinder the growth of this sector, which requires a comprehensive understanding to undertake the transformation of the sector.In  parallel,the  BIM  approach  has  proven  its  ability  to  provide  a  reliable  and  rich  enough  model  to perform  several  realistic  simulations  of  a  construction  project  since  the  design  phase  in  order  to support decision making processes. There is now an important offer insimulation software allowing stakeholders  to  test  different  construction  options  (material,  structural,  building  components,  etc.) from various  perspective  (cost,  carbon  footprint,  energy,  accessibility,  acoustics,  etc.)  at  the  design phase. Nevertheless, in the frame of the BIM4REN project, the initial version of the BIMfiles produced are most of the time very poor in terms of properties descriptions. Most of the time, only the structure and geometric aspect are present, which is not rich enough to envisage various renovation scenarios. The present tool will allow industrials to publish their cataloguesof products in a way that make themcompatible with the other tools developed along the value chain of BIM4REN. These catalogues will be then reused to enrich the BIM with products coming along with their properties.This deliverable is from Subtask T4.4.1: Object catalogueservers and backends integration.},
  langid = {english},
  keywords = {/unread}
}

@inproceedings{busFrenchSmartBuilding2018,
  title = {Towards {{French Smart Building Code}}: {{Compliance Checking Based}} on {{Semantic Rules}}},
  shorttitle = {Towards {{French Smart Building Code}}},
  booktitle = {Linked {{Data}} in {{Architecture}} and {{Construction}} ({{LDAC}} 2018)},
  author = {Bus, Nicolas and Roxin, Ana and Picinbono, Guillaume and Fahad, Muhammad},
  date = {2018-06-19},
  series = {{{CEUR-WS Proceedings Vol}}. 2159},
  pages = {6--15},
  location = {{London}},
  url = {https://hal.archives-ouvertes.fr/hal-02301861},
  urldate = {2021-05-17},
  abstract = {Manually checking models for compliance against building regulation is a time-consuming task for architects and construction engineers. There is thus a need for algorithms that process information from construction projects and report non-compliant elements. Still automated code-compliance checking raises several obstacles. Building regulations are usually published as human readable texts and their content is often ambiguous or incomplete. Also, the vocabulary used for expressing such regulations is very different from the vocabularies used to express Building Information Models (BIM). Furthermore, the high level of details associated to BIM-contained geometries induces complex calculations. Finally, the level of complexity of the IFC standard also hinders the automation of IFC processing tasks. Model chart, formal rules and pre-processors approach allows translating construction regulations into semantic queries. We further demonstrate the usefulness of this approach through several use cases. We argue our approach is a step forward in bridging the gap between regulation texts and automated checking algorithms. Finally with the recent building ontology BOT recommended by the W3C Linked Building Data Community Group, we identify perspectives for standardizing and extending our approach.},
  keywords = {Automated Compliance Checking (ACC),Building Information Modeling (BIM),construction regulations,France,ifcOWL,International Foundation Classes (IFC),OWL,semantic rules,SWRL}
}

