@article{hanEfficientBuildingEnergy2011,
  title = {Efficient {{Building Energy Management System Based}} on {{Ontology}}, {{Inference Rules}}, and {{Simulation}}},
  author = {Han, Jinsoo and Jeong, Youn-Kwae and Lee, Il-Woo},
  date = {2011},
  journaltitle = {International Proceedings of  Computer Science and Information Technology},
  url = {https://www.semanticscholar.org/paper/Efficient-Building-Energy-Management-System-Based-Han-Jeong/5b01595b73f0c2ecf0501f7c92d31fae6695254c},
  urldate = {2021-10-27},
  abstract = {Energy use in the building sector shares a great portion of world energy and accounts for a lot of green house gas. It is necessary to increase energy efficiency in the building sector to reduce energy cost and green house gas emissions. This paper proposes a building energy management system based on ontology, inference rules, and simulation. Ontology modeling has the advantages of information reusability, extensibility, and interoperability. It enables the BEMS to be easily deployed in the field. Building energy simulation helps the inference rules accumulated. The efficiency will be increased in proportional to the number of inference rules. The feasibility of the proposed BEMS model is shown through the pilot system. The proposed model is expected to greatly contribute to the energy saving and the reduction of green house gas emissions.},
  langid = {english},
  keywords = {/unread}
}

@inproceedings{hanRuleBasedOntologyReasoning2015,
  title = {A {{Rule-Based Ontology Reasoning System}} for {{Context-Aware Building Energy Management}}},
  booktitle = {2015 {{IEEE International Conference}} on {{Computer}} and {{Information Technology}}; {{Ubiquitous Computing}} and {{Communications}}; {{Dependable}}, {{Autonomic}} and {{Secure Computing}}; {{Pervasive Intelligence}} and {{Computing}}},
  author = {Han, Jinsoo and Jeong, Youn-Kwae and Lee, Ilwoo},
  date = {2015-10},
  pages = {2134--2142},
  doi = {10.1109/CIT/IUCC/DASC/PICOM.2015.317},
  abstract = {Global energy demand grows rapidly. The building sector accounts for about 20\% of world's energy consumption. Intelligent and systematic energy management technologies have been increasingly exploited to reduce the energy use in the building sector. Previous studies used their own decision model including knowledge-bases and rules. However, they are lack of a structural framework for organizing information and a form of knowledge representation as a set of concepts in the building energy domain. To enhance a framework and knowledge representation, we adopt ontology. Furthermore, ontology can provide reasoning functionality and context-awareness. We propose a rule-based ontology reasoning system that provides context-awareness. In the proposed system, we design an ontology model for building energy management and make reasoning rules from the knowledge-bases of accumulated building energy saving measures. The proposed system detects energy waste context and provides energy saving measures to eliminate the energy waste context. We implemented the proposed system on a real office building. The implemented ontology reasoning system diagnoses the context of the target building and presents the energy saving measures simultaneously. By use of building energy simulation, the amount of energy saving is estimated when the energy saving measures are applied. The simulation result shows the positive effect of our proposed rule-based ontology reasoning system.},
  eventtitle = {2015 {{IEEE International Conference}} on {{Computer}} and {{Information Technology}}; {{Ubiquitous Computing}} and {{Communications}}; {{Dependable}}, {{Autonomic}} and {{Secure Computing}}; {{Pervasive Intelligence}} and {{Computing}}},
  keywords = {Building Energy Management,Buildings,Cognition,Context,Context modeling,Context-Aware,Energy management,Energy Saving,Knowledge based systems,Ontologies,Ontology,Reasoning,Uses GraphDB}
}

