@incollection{zhangBuildingInformationModeling2022,
  title = {Building Information Modeling, Natural Language Processing, and Artificial Intelligence for Automated Compliance Checking},
  booktitle = {Research {{Companion}} to {{Building Information Modeling}}},
  author = {Zhang, Ruichuan and El-Gohary, Nora},
