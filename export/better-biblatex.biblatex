@online{gabrielfierroOndemandWebinarNew2022,
  title = {On-Demand Webinar: {{A}} New Standard Is Now Available to Use for Harmonization between Two Smart Building Metadata Standards},
  shorttitle = {On-Demand Webinar},
  author = {{Gabriel Fierro} and {Karl Hammar} and {Joel Bender} and {Erik Paulson} and {Akshay Johar} and {Erik Wallin} and {Erik Wallin}},
  date = {2022-08-31T08:42:48+00:00},
