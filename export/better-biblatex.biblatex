@article{oraskariComputationallyEfficientIFC2015,
  title = {Computationally {{Efficient IFC File Comparison Using RDF Graph Paths}}},
  author = {Oraskari, J. and Törmä, S.},
  date = {2015},
  journaltitle = {undefined},
  url = {https://www.semanticscholar.org/paper/Computationally-Efficient-IFC-File-Comparison-Using-Oraskari/6d95ac63eaace659b21056893d73b6c5c5d39774},
  urldate = {2022-11-23},
  abstract = {An algorithm that utilizes the neighborhood of an anonymous node to find a probable matching node pair in the second version of the model to achieve a balance between the certainty of the matching pair and the size of the neighborhood can be found. We study computationally efficient methods calculating changes in IFC models that have been converted to RDF graphs. It is shown how to calculate difference sets that are small enough to be descriptive and usable for synchronization while still using limited computing resources. We present an algorithm that utilizes the neighborhood of an anonymous node to find a probable matching node pair in the second version of the model. By limiting the neighborhood only to the most informative paths in the RDF graph, a balance between the certainty of the matching pair and the size of the neighborhood can be found. The algorithm is compared with commonly available RDF diff tools: Jena, and RDFContextTools RDFC14Ner. When compared, our algorithm provided significantly better results.},
  langid = {english},
  keywords = {/unread}
}

@report{oraskariD4BIM4RENRepository2022,
  type = {Deliverable},
  title = {D4.2 - {{BIM4REN}} Repository: Software and User Guide},
  author = {Oraskari, Jyrki and Bourreau, Pierre and Böhms, Michel},
  date = {2022-09-21},
  number = {BIM4Ren - D4.2},
  pages = {42},
  abstract = {Building information modelling is often defined in different ways, the two main ones being either a collaborative process between project stakeholders, and the digitial building model that supports it.In the BIM4Ren project, the consortium is looking at how to use a digital avatar of a building to improve the energy performance of the renovated building, the cost of the works, the timings, etc. In order to do so, different information need to be inserted in the BIM model that is used by all staekholders. In D2.1, we already discussed different options that can be used to model buildings, from traditional IFC step files, to the more recently adopted linked data formats. Pros and cons of each solutions in the context  of  a  renovation  scenario  were  discussed,  and  a  first  draft  of  a  linked  data  approach  was elaborated.In  the  present  document,  we  present  the  BIM4Ren  ontology  and  the  main  concepts  used.  The ontology is a set of different ontologies, that are fully modular, i.e. for each project, different modules can be used and left appart. This works is the BIM4Ren proposal to modularising existing standards, and  more  particularly  IFC,  an  issue  that  is  currently  under  study  by  the  buildingSmart  technical community.  In  the  current  deliverable,  we  also  present  someconversion  procedures,  to  ease interoperability  between  different  formats.  The  BIM4Ren  ontology  is  indeed  referencing  concepts from Omniclass, or IFC itself, and we show two different ways to convert building models from IFC to BIM4Ren.The first approachis based on inference rules, that are interpreted by a reasoner embedded in a triple store.We  discussed  its  implementation,  scalability  and  limitations.  The  second  one  is  a  Java-based program, implemented as a BIM Bot.With the present document, the BIM model storage facilities are covered.},
  keywords = {/unread,BIM4REN project}
}

@inproceedings{oraskariDescribingVersionHistory2022,
  title = {Towards Describing Version History of {{BCF}} Data in the {{Semantic Web}}},
  booktitle = {Linked {{Data}} in {{Architecture}} and {{Construction}} ({{LDAC}} 2022)},
  author = {Oraskari, Jyrki and Schulz, Oliver and Beetz, Jakob},
  date = {2022-05},
  url = {https://linkedbuildingdata.net/ldac2022/files/papers/paper08.pdf},
  abstract = {The buildingSMARTs BIM Collaboration Format (BCF) is a vendor-neutral standard to communicate BIM-based issues. The aspirations are well in line with open BIM- and the linked building data (LBD) community to use open data standards in Architecture, Engineering and Construction industry. Our previous research introduced the bcfOWL ontology to express BCF data using the open Web of Data standards of the World Wide Web Consortium (W3C) as part of the Linked Building Data (LBD). The current BCF API standard expresses changes in the model as event logs that indirectly describe the modifications. The focus is on different changes issue descriptions are going through in the course of a life cycle of a building. This paper presents four different approaches for expressing the temporal changes as Linked Data. Two RDF-star approaches and a state construction inspired by the Ontology for Property Management (OPM) are introduced. Moreover, we show Event System method that is close to the original BCF API. To compare the approaches, queries to get the current data and search the history are provided on an external repository. Finally, the results and their adaptation for LBD, in general, are assessed.},
  venue = {Hersonissos, Greece},
  keywords = {/have,/unread,BIM Collaboration Format (BCF),RDF-star,version history}
}

@inproceedings{oraskariDynamicBIMFormat2021,
  title = {Dynamic {{BIM}} Format Conversion as Inference-Based Ontology Alignment},
  author = {Oraskari, Jyrki and Bourreau, Pierre},
  date = {2021},
  url = {http://ceur-ws.org/Vol-3081/11paper.pdf},
  urldate = {2022-10-26},
  abstract = {The AEC sector is known to be highly fragmented, different experts requiring different information for their works. This is also reflected with the existence of different domain formats, such as gbXML for energy modelling. Current practices can be described as static as they are based file exchange and conversion, mainly using the IFC format as an intermediate. In an ideal BIM level 3 platform, different stakeholders could work together on the same building model, using different vocabularies, and update the model synchronously. We propose an approach based on semantic rule reasoning to dynamically convert information in different vocabularies. The approach is implemented and tested on converting models from ifcOwl to BIM4Ren, a BOT-based ontology. Our work extends original alignments between ifcOwl and BOT to produce BOT relations. The scalability of the implementation is discussed as well as its limitation.},
  eventtitle = {Linked {{Data}} in {{Architecture}} and {{Construction}} ({{LDAC}} 2021)},
  langid = {english},
  keywords = {/unread,BIM4REN project,Building Topology Ontology (BOT),Energy Management,gbXML,ifcOWL,International Foundation Classes (IFC),ontology alignment,{Project Management, Cost Estimation, Change Management},Semantic alignment,semantic conversion}
}

@inproceedings{oraskariEnablingFederatedInteroperable2022,
  title = {Enabling {{Federated Interoperable Issue Management}} in a {{Building}} and {{Construction Sector}}},
  booktitle = {Proceedings of the 29th {{EG-ICE International Workshop}} on {{Intelligent Computing}} in {{Engineering}}},
  author = {Oraskari, J. and Schulz, O. and Werbrouck, J. and Beetz, J.},
  date = {2022-06-20},
  pages = {92--101},
  publisher = {{EG-ICE}},
  doi = {10.7146/aul.455.c200},
  url = {https://ebooks.au.dk/aul/catalog/view/455/312/1848-2},
  urldate = {2022-10-12},
  abstract = {A Common Data  Environment (CDE)is an  agreed-upon  source  of  information  on building-related projectsto collect, manage,and exchange data between stakeholders. Theapproach in  the  AEC  domain is  to  usebuildingSMART’sBIM  Collaboration  Format  (BCF)as thedigital issue  communication part  ofCDEs.Contrasting  with  the  federated  nature  of  the  AEC  industry, CDEs are typically organised in a centralised fashion.This work proposes a potential transition of BCF  into  a  distributed  environment  that servesas  an  example  for  further  developments inthe distribution of CDEs and CDE-independent data management. We show how a single source of truth over the project data and the advantages of the central approach can be realised in a distributed setupusing a Solid  architecture  environment,enablingdecentralised  authentication  and stakeholders’ability to control their data.},
